extends layouts/main-layout.pug

block styles
    <link rel="stylesheet" href="/css/user.css">
block content   
    <main>
        if users.length > 0
            <ul>
                each user in users
                    <li class="user">#{ user.user }</li>
            </ul>
        else
         <h1>No Users</h1>    
    </main>